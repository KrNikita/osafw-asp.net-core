    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "sessionIdleTimeout": 120,
  "cookieHttpOnly": true,
  "appSettings": {
    "db": {
      "main": {
        "connection_string": "Data Source=(local);Initial Catalog=demo;Integrated Security=True",
        "type": "SQL"
