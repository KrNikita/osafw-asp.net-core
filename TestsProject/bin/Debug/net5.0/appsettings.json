    "route_prefixes": {
      "/Sys": 1,
      "/Admin": 1,
      "/My": 1,
      "/Dev": 1
    },
    "PAGE_LAYOUT": "/layout_fluid.html",
    "PAGE_LAYOUT_PUBLIC": "/layout.html"
  }
}
